{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "stripInternal":true,
    "removeComments":true,
    "importHelpers":false,
    "strictNullChecks":true,
    "noImplicitAny":true,
    "noImplicitReturns":true,
    "noImplicitThis":true,
    "noImplicitUseStrict":true,
    "lib": ["dom", "es2015.promise", "es5"]
  },
  "include":[
    "src/**/*"
  ]
}