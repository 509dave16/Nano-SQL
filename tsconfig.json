{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "stripInternal":true,
    "removeComments":true,
    "importHelpers":false,
    "strictNullChecks":true,
    "noImplicitAny":true,
    "noImplicitReturns":true,
    "noImplicitThis":true,
    "noImplicitUseStrict":true
  },
  "include":[
    "src/**/*"
  ],
  "exclude":[
    "src/old-memory-db.ts",
    "src/immutable-db.ts"
  ]
}