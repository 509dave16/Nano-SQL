{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "stripInternal":true,
    "removeComments":true,
    "importHelpers":false,
    "strictNullChecks":true,
    "noImplicitAny":true,
    "noImplicitReturns":true,
    "noImplicitThis":true
  },
  "exclude":[
    "examples",
    "node_modules"
  ]
}